{
  "kind": "collectionType",
  "collectionName": "clientes",
  "info": {
    "name": "Clientes"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Nome": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "E_mail": {
      "type": "email",
      "required": true,
      "unique": true,
      "minLength": 5
    },
    "Data_de_Nascimento": {
      "type": "date",
      "required": true
    }
  }
}
